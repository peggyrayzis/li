{
  "name": "@peggyrayzis/li",
  "version": "0.1.3",
  "description": "The LinkedIn CLI for agents",
  "type": "module",
  "packageManager": "pnpm@9.1.1",
  "bin": {
    "li": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsc --watch",
    "li": "node dist/cli.js",
    "test": "vitest run",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "security": "bash scripts/security-check.sh",
    "check": "npm run lint && npm run typecheck && npm run test",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "hooks:install": "bash scripts/install-githooks.sh",
    "prepare": "npm run hooks:install",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "linkedin",
    "cli",
    "voyager",
    "api"
  ],
  "author": "Peggy Rayzis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peggyrayzis/li.git"
  },
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@steipete/sweet-cookie": "^0.1.0",
    "commander": "^14.0.3",
    "dotenv": "^17.2.3",
    "json5": "^2.2.3",
    "picocolors": "^1.1.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "@steipete/sweet-cookie@0.1.0": "patches/@steipete__sweet-cookie@0.1.0.patch"
    }
  }
}
